{
  "name": "@abc-map/server",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "main": "build/main.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf build",
    "clean-build": "yarn run clean && yarn run build",
    "coverage": "nyc mocha --trace-warnings 'build/**/*.test.js'",
    "dep-check": "depcruise -c .dependency-cruiser.js src",
    "lint": "eslint './src/**/*.ts' --max-warnings=0",
    "lint-fix": "yarn run lint --fix",
    "start:ci": "node ./build/main.js",
    "start:watch": "nodemon --trace-warnings --watch build --watch resources ./build/main.js",
    "test": "mocha --trace-warnings 'build/**/*.test.js'",
    "test:interactive": "mocha --watch --trace-warnings 'build/**/*.test.js'",
    "watch": "tsc --watch | sed -u 's/\\x1Bc//g'"
  },
  "dependencies": {
    "@abc-map/shared": ">1.0.0-rc",
    "@fastify/error": "^3.2.0",
    "@fastify/jwt": "^6.5.0",
    "@fastify/multipart": "^7.3.0",
    "@fastify/rate-limit": "^7.6.0",
    "@fastify/sensible": "^5.2.0",
    "@fastify/static": "^6.6.0",
    "@fastify/view": "^7.3.0",
    "accept-language-parser": "^1.5.0",
    "ajv": "8.11.2",
    "bson": "^4.7.0",
    "fastify": "4.10.2",
    "fastify-metrics": "^10.0.1",
    "glob": "^8.0.3",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "luxon": "^3.1.1",
    "mongodb": "4.13.0",
    "nodemailer": "^6.8.0",
    "prom-client": "^14.1.0",
    "source-map-support": "^0.5.21",
    "uuid-random": "^1.3.2"
  },
  "devDependencies": {
    "@types/accept-language-parser": "^1.5.3",
    "@types/chai": "^4.3.4",
    "@types/geojson": "^7946.0.10",
    "@types/glob": "^8.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.17",
    "@types/nodemailer": "^6.4.7",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "chai": "^4.3.7",
    "dependency-cruiser": "^12.1.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "form-data": "^4.0.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "prettier": "^2.8.1",
    "sinon": "^15.0.1",
    "typescript": "^4.9.4"
  },
  "volta": {
    "node": "18.12.1"
  },
  "docker": {
    "imageName": "server"
  }
}
